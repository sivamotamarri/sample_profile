<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
  <head>
    <title><%= @page_title  %></title>
    <meta http-equiv="content-type" content="text/html; charset=utf-8" />
    <%= javascript_include_tag :defaults %>
   
    <%= stylesheet_link_tag 'style' %>
    
<!--[if IE 6]>
<style>
.black_overlay{
height:2000px;

}
</style>
<![endif]-->
  </head>
  <body>

    <div id="Container">
      <div id="HeaderContainer">
<div style="width:1000px; height:60px; font-size: 24px;background-color:#3C4C72; color:#FFFFFF;">
<center><div style="padding-top:10px;">Welcome to MyProfile</div></center></div>
 <div style="height:10px;"></div>
        <div id="HeaderLinkLogin" class="HeaderLinkText">
          <span>
            
			<% if session[:user_id] then %>
            <%= link_to("Home", {:controller=>'home', :action=>'index'},:class => "HeaderLink") %> |
            <%= link_to("Myprofile", {:controller=>'home', :action=>'show'},:class => "HeaderLink") %>| 
			<%= link_to "Logout" ,:controller => "login" ,:action => "logout" %> 
			<% end %>
          </span><br /><br />

         
        </div>

      

       
      </div>
      <!-- end header -->

      <div id="BodyContainer">
        <div id="Center">
          <div id="PgHeadBg">
            <div id="PgHeadingWithLink">
              <%# if logged_in? %>
                <%#=# tabnav controller.subtab_nav %>
              <%# end %>

            </div>
          </div>

          <!-- mainContent -->
          <div id="CenterContainer" style="border:0px solid #000000;">
		  
            <%= yield %>
          </div><!-- end mainContent -->

          <div style="clear:both;"></div>
<br/>
          <div id="Footer">
            <div id="FooterDivs">
              <div id="FooterLeft">Copyright Â© 2009  All Rights Reserved.   </div>
            </div>
          </div>
        </div>
      </div>
      <div><img src="/images/footerborder.gif" alt="" border="0" /></div>
    </div>

    <!-- end #container -->

  </body>
</html>
